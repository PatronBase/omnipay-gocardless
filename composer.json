{
    "name": "patronbase/omnipay-gocardless",
    "type": "library",
    "description": "GoCardless payment gateway driver for the Omnipay 3.x payment processing library",
    "keywords": [
        "go cardless",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "direct debit"
    ],
    "homepage": "https://github.com/PatronBase/omnipay-gocardless",
    "license": "MIT",
    "authors": [
        {
            "name": "Dan Eastmond",
            "email": "dan@firesoft.tech"
        },
        {
            "name": "Omnipay Contributors",
            "homepage": "https://github.com/thephpleague/omnipay-braintree/contributors",
            "role": "Original Base Package"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\GoCardless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\GoCardless\\": "tests/"
        }
    },
    "require": {
        "omnipay/common": "^3",
        "symfony/property-access": "^3.2 || ^4.0",
        "gocardless/gocardless-pro": "^1",
        "php": "^5.5|^7",
        "symfony/event-dispatcher": "^2.8"
    },
    "require-dev": {
        "omnipay/tests": "^3",
        "squizlabs/php_codesniffer": "^3"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "3.0.x-dev"
        }
    }
}
