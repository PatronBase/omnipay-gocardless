{
    "name": "patronbase/omnipay-gocardless",
    "type": "library",
    "description": "GoCardless payment gateway driver for the Omnipay payment processing library",
    "keywords": [
        "gocardless",
        "go cardless",
        "gateway",
        "merchant",
        "omnipay",
        "pay",
        "payment",
        "direct debit",
        "redirect"
    ],
    "homepage": "https://github.com/PatronBase/omnipay-gocardless",
    "license": "MIT",
    "authors": [
        {
            "name": "Leith Caldwell",
            "email": "leith@codedruids.com"
        },
        {
            "name": "Dan Eastmond",
            "email": "dan@firesoft.tech"
        }
    ],
    "autoload": {
        "psr-4": {
            "Omnipay\\GoCardless\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Omnipay\\GoCardless\\": "tests/"
        }
    },
    "require": {
        "omnipay/common": "~3.3",
        "php": "^5.5|^7|^8"
    },
    "require-dev": {
        "omnipay/tests": "dev-address3-support",
        "squizlabs/php_codesniffer": "^3.5",
        "symfony/psr-http-message-bridge": "~1.1.0"
    },
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/CodeDruids/omnipay-tests"
        }
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "3.1.x-dev"
        }
    },
    "prefer-stable": true
}
